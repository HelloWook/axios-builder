{
  "name": "@wookgi/axios-builder",
  "version": "1.0.5",
  "description": "A builder pattern implementation for creating API requests using Axios in TypeScript",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.umd.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.umd.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:tsc && npm run build:esbuild",
    "dev": "ts-node example.ts",
    "build:tsc": "tsc",
    "build:esbuild": "ts-node esbuild.config.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "axios": "^1.12.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.4.0",
    "esbuild": "^0.25.9",
    "jest": "^30.1.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
